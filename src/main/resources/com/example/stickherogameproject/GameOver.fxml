<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="308.0" prefWidth="437.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.stickherogameproject.HelloController">
   <children>
      <ImageView fitHeight="308.0" fitWidth="437.0" layoutX="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@GameOver.jpg" />
         </image>
      </ImageView>
      <Button layoutX="128.0" layoutY="193.0" mnemonicParsing="false" onAction="#switchToScene1" prefHeight="25.0" prefWidth="56.0" style="-fx-background-color: #eb4d47;" text="Home" textFill="#241f1f">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button layoutX="184.0" layoutY="168.0" mnemonicParsing="false" onAction="#switchToScene2" style="-fx-background-color: #eb4d47;" text="Restart" textFill="#220707">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button fx:id="Revive" layoutX="240.0" layoutY="193.0" mnemonicParsing="false" onAction="#reviveGame" style="-fx-background-color: #eb4d47;" text="Revive" textFill="#220302">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
